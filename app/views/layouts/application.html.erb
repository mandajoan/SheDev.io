<!DOCTYPE html>
<html>
  <head>
    <title>SheDevApp</title>
    <%= csrf_meta_tags %>

    <%= stylesheet_link_tag    'application', media: 'all', 'data-turbolinks-track': 'reload' %>
    <%= javascript_include_tag 'application', 'data-turbolinks-track': 'reload' %>



  </head>

  <body>
    <header>
      <% if logged_in %>
      <%= image_tag "pinkHeader.png", :id => "headerLogo"%>
      <% end %>
    </header>

    <% if notice %>
        	    <%= notice %>
  	<% end %>

  	<% if alert %>
        	    <%= alert %>
  	<% end %>

    <% if logged_in %>
    <%= render 'layouts/navbar' %>
    <% end %>
    <%= yield %>

  </body>

  <footer>
    <% if logged_in %>
  <%= image_tag "newfooter.png", :id => "footerLogo"%>
  <% end %>
  </footer>
</html>
