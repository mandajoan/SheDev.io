
<div class="row" id="loginPageDiv">

<div class="col-md-3">

 <br>
 <br>
 <br>
 <br>
 <br>
    <h1>Login </h1>

    <%= form_for :login, {url: :login} do |f| %>
    <br>
    <%= f.email_field :email, placeholder: "Email" %>
    <br>
    <%= f.password_field :password, placeholder: "Password" %>
    <br>
    <br>
    <%= f.submit 'Log In' %>
    <% end %>


</div>
<div class="col-md-6">
<%= image_tag "logo2.png" %>
</div>
</div>
